{
  "name": "sbsitnstay-backend-06-2018",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "nodemon server.js",
    "front": "webpack-dev-server --watch",
    "back": "node server.js",
    "prebuild-css": "shx cp -R node_modules/bootstrap/scss src/bootstrap",
    "build-css": "node-sass-chokidar ./src/assets/sass/main.scss ./src/assets/css/main.css --watch",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "dev": "concurrently \"npm run back\" \"npm run prebuild-css\" \"npm run build-css\" \"npm run watch-css\" \"npm run front\""
  },
  "dependencies": {
    "axios": "^0.18.0",
