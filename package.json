    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.6",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "http-errors": "~1.6.2",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.4",
    "morgan": "~1.9.0",
